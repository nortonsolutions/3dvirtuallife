<table class="table table-striped table-inverse table-responsive">
    <thead class="thead-inverse">
        <tr>
            <th>Game Name</th>
            <th>Hero Name</th>
            <th>Level</th>
        </tr>
        </thead>
        <tbody>
            {{#each this}}
            <tr>
                <td scope="row">{{gameName}}</td>
                <td>{{heroName}}</td>
                <td>{{level}}</td>
                <td><button type="button" name="{{gameName}}" id="{{gameName}}" class="btn btn-primary btn-block btn-sm load"><i class="fas fa-box-open noClick"></i></button></td>
                <td><button type="button" name="{{gameName}}" id="{{gameName}}" class="btn btn-danger btn-block btn-sm delete"><i class="fas fa-minus noClick"></i></button></td>
            </tr>
            {{/each}}
        </tbody>
</table>
